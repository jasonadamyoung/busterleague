<table class="table table-bordered">
  <thead>
    <tr>
        <th>Filename</th>
        <th>Size</th>
        <th>Uploaded At</th>
        <th>Uploaded By</th>
    </tr>
  </thead>

  <tbody>
    <%- @stat_sheets.each do |stat_sheet| -%>
        <tr>
          <td><%= link_to(stat_sheet.sheet.metadata['filename'],stat_sheet.sheet_url) %></td>
          <td><%= humanize_bytes(stat_sheet.sheet_size) %></td>
          <td><%= stat_sheet.created_at %></td>
          <td><%= stat_sheet.owner.nickname -%></td>
        </tr>
    <%- end -%>
  </tbody>
</table>
