<table class="table table-bordered ">
	 <tbody>
    <%- (DraftStatDistribution::BATTING_DIRECTIONS.keys - DraftStatDistribution.core(DraftStatDistribution::BATTER) - ['age']).sort.each do |stat| -%>
	    <tr>
		    <td><%= stat %></td>
		    <td><%= @player.statline.send(stat) %></td>
	    </tr>
	  <%- end -%>
 </tbody>
</table>
