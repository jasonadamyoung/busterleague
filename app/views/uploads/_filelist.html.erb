<table class="table table-bordered">
  <thead>
    <tr>
        <th>Filename</th>
        <th>Size</th>
        <th>Uploaded At</th>
        <th>Uploaded By</th>
        <th>Status</th>
    </tr>
  </thead>

  <tbody>
    <%- @uploads.each do |upload| -%>
        <tr>
          <td><%= upload.archive.metadata['filename'] %></td>
          <td><%= humanize_bytes(upload.archive_size) %></td>
          <td><%= upload.created_at %></td>
          <td><%= upload.owner.nickname -%></td>
          <td><%= upload.processing_status_to_s -%></td>
        </tr>
    <%- end -%>
  </tbody>
</table>
